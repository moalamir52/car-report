{
  "manifest_version": 3,
  "name": "Sheets Writer",
  "version": "1.0",
  "permissions": [
    "https://script.google.com/*"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],

  "content_scripts": [
    {
      "matches": ["http://localhost:3000/*"],
      "js": ["content.js"]
    }
  ],

}